		"discord.js": "^14.22.1",
		"knip": "^5.63.1",
		"markdown-toc": "^1.2.0",
		"publint": "^0.3.13",
		"tsc-alias": "^1.8.16",
		"tsdown": "^0.15.2",
		"typescript": "^5.9.2"
