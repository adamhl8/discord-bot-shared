  "type": "module",
  "description": "Modules for creating discord bots.",
  "repository": {
    "type": "git",
    "url": "https://github.com/adamhl8/discord-bot-shared"
  },
  "homepage": "https://github.com/adamhl8/discord-bot-shared",
  "bugs": {
