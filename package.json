    "prepublish": "bun bundle"
  },
  "peerDependencies": {
    "discord.js": "^14.0.0"
  },
  "devDependencies": {
    "@types/bun": "^1.2.2",
    "discord.js": "^14.18.0",
    "eslint": "^9.20.1",
    "eslint-config-builder": "^0.17.0",
    "prettier": "^3.5.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1"
  }
